!
version 15.2
no service pad
service tcp-keepalives-in
service tcp-keepalives-out
service timestamps debug datetime msec localtime show-timezone
service timestamps log datetime msec localtime show-timezone
service password-encryption
service compress-config
no service dhcp
!
hostname {{ site prefix }}-vsa-{{ local-id }}
!
boot-start-marker
boot system switch all flash:cat3k_caa-universalk9.SPA.03.07.02.E.152-3.E2.bin
boot system switch all flash:
boot-end-marker
!
!
vrf definition Mgmt-vrf
 !
 address-family ipv4
 exit-address-family
 !
 address-family ipv6
 exit-address-family
!
logging userinfo
logging buffered 1024000 notifications
no logging console
enable secret {{ local enable password }}
!
username failsafe secret {{ local password }}
aaa new-model
!
!
aaa group server tacacs+ GTAC
 !
 ! Specify "port {{ TCP port from spreadsheet }}" in the following commands if TCP port is other than the default 49
 !
 server-private {{ GTAC server 1 IP address from spreadsheet }} < port {{ TCP port from spreadsheet }} > timeout 6 key {{ GTAC administrator assigned key }}
 server-private {{ GTAC server 2 IP address from spreadsheet }} < port {{ TCP port from spreadsheet }} > timeout 6 key {{ GTAC administrator assigned key }}
 !
 ip tacacs source-interface Vlan{{ 1900+i }}
!
aaa authentication attempts login 5
aaa authentication fail-message ^Failed login. Five consecutive fails will revoke.^
aaa authentication login default group GTAC local
aaa authentication enable default group GTAC enable
aaa authorization console
aaa authorization config-commands
aaa authorization exec default group GTAC if-authenticated 
aaa authorization commands 15 default group GTAC none 
aaa accounting exec default start-stop group GTAC
aaa accounting commands 15 default start-stop group GTAC
aaa accounting connection default start-stop group GTAC
aaa accounting system default start-stop group GTAC
no aaa accounting system guarantee-first
!
!
!
!
!
!
aaa session-id common
switch 1 provision ws-c3650-{{ 24pd|48pd }}
!
!
!
!
!
no ip source-route
!
no ip domain-lookup
ip domain-name attmgmt.ibm.com
ip name-server {{ DNS server 1 IP address from spreadsheet }}
ip name-server {{ DNS server 2 IP address from spreadsheet }}
!
!
login on-failure log
login on-success log
ipv6 nd raguard policy pp6_raguard_client
!
ipv6 nd raguard policy pp6_raguard_trust
 trusted-port
!
ipv6 dhcp guard policy pp6_dhcp_client
ipv6 dhcp guard policy pp6_dhcp_trust
 trusted-port
qos queue-softmax-multiplier 100
vtp domain {{ site prefix }}
vtp mode transparent
!
!
errdisable recovery cause udld
errdisable recovery interval 180
diagnostic bootup level complete
archive
 log config
  logging enable
  notify syslog contenttype plaintext
  hidekeys
!
spanning-tree mode rapid-pvst
spanning-tree extend system-id
spanning-tree vlan 1700-1799,1901-4000 priority 8192
spanning-tree vlan 1700-1799,1901-4000 forward-time 6
spanning-tree vlan 1700-1799,1901-4000 max-age 8
hw-switch switch 1 logging onboard message level 3
!
redundancy
 mode sso
!
! Repeat the following yellow edge VLAN definition (v=10-19) for each matching VLAN required
!
vlan {{ 1700+v }}
 name vSA{{ i }}/yellow-edge{{ v-9 }}/{{ zone description }}
!
! Repeat the following green edge VLAN definition (v=20-29) for each matching VLAN required
!
vlan {{ 1700+v }}
 name vSA{{ i }}/green-edge{{ v-19 }}/{{ zone description }}
!
! Repeat the following blue edge VLAN definition (v=30-39) for each matching VLAN required
!
vlan {{ 1700+v }}
 name vSA{{ i }}/blue-edge{{ v-29 }}/{{ zone description }}
!
vlan {{ 1900+i }}
 name {{ vSD{{ n }}|vOD{{ n }}|eCD|eND }}-vSA{{ i }}/global/xA_mgmt
!
! Apply the following section for vSD VLAN type profile 2, if used on connected vSD
!
! Repeat the following blue data VLAN definition (v=0-19) for each matching VLAN required
!
vlan {{ 2000+20*v+q2+i }}
 name vSD{{ n }}-vSA{{ i }}/blue-{{ v+1 }}</{{ local description }}>
!
! Repeat the following green/yellow/gray data VLAN definition (v=20-39) for each matching VLAN required
!
vlan {{ 2000+20*v+q2+i }}
 name vSD{{ n }}-vSA{{ i }}/{{ green|yellow|gray }}-{{ v-19 }}/{{ zone description }}
!
! Repeat the following blue data VLAN definition (v=40-44) for each matching VLAN required
!
vlan {{ 2000+20*v+q2+i }}
 name vSD{{ n }}-vSA{{ i }}/blue-{{ v+1 }}</{{ local description }}>
!
! Repeat the following backend VLAN definition (v=45-46) for each matching VLAN required
!
vlan {{ 2000+20*v+q2+i }}
 name vSD{{ n }}-vSA{{ i }}/back-{{ v-44 }}</{{ local description }}>
!
! Apply the following global device management VLAN definition (v=49), if required
!
vlan {{ 2000+20*v+q2+i }}
 name vSD{{ n }}-vSA{{ i }}/global-1/dev_mgmt
!
! End of section for vSD VLAN type profile 2
!
! Apply the following section for vOD-eCD-eND VLAN type profile, if used on connected vOD, eCD or eND
!
! Repeat the following blue data VLAN definition (v=0-4) for each matching VLAN required
!
vlan {{ 2000+20*v+q2+i }}
 name {{ vOD{{ n }}|eCD|eND }}-vSA{{ i }}/blue-{{ v+1 }}</{{ local description }}>
!
! Repeat the following green/yellow/gray data VLAN definition (v=15-19) for each matching VLAN required
!
vlan {{ 2000+20*v+q2+i }}
 name {{ vOD{{ n }}|eCD|eND }}-vSA{{ i }}/{{ green|yellow|gray }}-{{ v-14 }}/{{ zone description }}
!
! Apply the following global device management VLAN definition (v=24), if required
!
vlan {{ 2000+20*v+q2+i }}
 name {{ vOD{{ n }}|eCD|eND }}-vSA{{ i }}/global-1/dev_mgmt
!
! End of section for vOD-eCD-eND VLAN type profile
!
vlan 4094
 name vSA/Null
lldp run
cdp timer 10
cdp holdtime 30
!
ip tcp path-mtu-discovery
ip ssh version 2
ip ssh dscp 18
!
class-map match-any cm_att_ipsla_class1
 match access-group name al_att_ipsla_class1
class-map match-any cm_att_ipsla_class3
 match access-group name al_att_ipsla_class3
class-map match-any cm_att_ipsla_class2
 match access-group name al_att_ipsla_class2
class-map match-any cm_att_forward_class3
 match access-group name al_att_forward_class3
class-map match-any cm_att_mgmt_return_class2
 match access-group name al_att_mgmt_return_class2
class-map match-any cm_att_mgmt_return_class3
 match access-group name al_att_mgmt_return_class3
class-map match-any cm_att_class1
 match access-group name al_att_class1
class-map match-any cm_att_voip_return_class2
 match access-group name al_att_voip_return_class2
class-map match-any cm_att_voip_forward_class2
 match access-group name al_att_voip_forward_class2
class-map match-any cm_att_data_forward_class2
 match access-group name al_att_data_forward_class2
class-map match-any cm_att_mgmt_forward_class3
 match access-group name al_att_mgmt_forward_class3
class-map match-any cm_att_mgmt_forward_class2
 match access-group name al_att_mgmt_forward_class2
class-map match-any non-client-nrt-class
class-map match-any cm_att_data_return_class2_user
 match access-group name al_att_data_return_class2_user
class-map match-any cm_att_return_class3
 match access-group name al_att_return_class3
class-map match-any cm_dscp_att_class2
 match ip dscp af31  af32 
class-map match-any cm_dscp_att_class3
 match ip dscp af21  af22 
class-map match-any cm_dscp_att_class1
 match ip dscp ef 
class-map match-any cm_att_data_return_class2_server
 match access-group name al_att_data_return_class2_server
!
policy-map pm_att_classes_data
 class cm_att_data_forward_class2
  set dscp af31
 class cm_att_data_return_class2_server
  set dscp af31
 class cm_att_data_return_class2_user
  set dscp af31
 class cm_att_forward_class3
  set dscp af21
 class cm_att_return_class3
  set dscp af21
 class class-default
  set dscp default
policy-map pm_trust_dscp_att_cos
 class cm_dscp_att_class1
  set cos 5
 class cm_dscp_att_class2
  set cos 3
 class cm_dscp_att_class3
  set cos 2
 class class-default
  set cos 0
policy-map pm_att_queuing
 class cm_dscp_att_class1
  priority level 1
 class cm_dscp_att_class2
  bandwidth remaining percent 25 
 class cm_dscp_att_class3
  bandwidth remaining percent 25 
 class class-default
  bandwidth remaining percent 25 
policy-map pm_att_classes_voip_data
 class cm_att_class1
  set dscp ef
 class cm_att_voip_forward_class2
  set dscp af31
 class cm_att_data_forward_class2
  set dscp af31
 class cm_att_voip_return_class2
  set dscp af31
 class cm_att_data_return_class2_server
  set dscp af31
 class cm_att_data_return_class2_user
  set dscp af31
 class cm_att_forward_class3
  set dscp af21
 class cm_att_return_class3
  set dscp af21
 class class-default
  set dscp default
policy-map port_child_policy
 class non-client-nrt-class
  bandwidth remaining ratio 10
policy-map pm_att_classes_mgmt
 class cm_att_mgmt_forward_class2
  set dscp af31
 class cm_att_mgmt_return_class2
  set dscp af31
 class cm_att_mgmt_forward_class3
  set dscp af21
 class cm_att_mgmt_return_class3
  set dscp af21
 class class-default
  set dscp default
policy-map pm_att_classes_ipsla_data
 class cm_att_ipsla_class1
  set dscp ef
 class cm_att_ipsla_class2
  set dscp af31
 class cm_att_ipsla_class3
  set dscp af21
 class class-default
  set dscp default
!
!
!
!
!
!
!
!
!
!
!
!
!
!
interface GigabitEthernet0/0
 description ** RESERVED **
 vrf forwarding Mgmt-vrf
 no ip address
 shutdown
 negotiation auto
!
! Apply one of the following interface definitions for each port 1-23|47 in chassis 1
!
! Port option for chassis 1 - single-connected servers (non-trunked), if any
!
interface GigabitEthernet1/0/{{ 1-23|47 }}
 description {{ server name }}/{{ cable ID }}/{{ VRF name }}</{{ local description }}>
 switchport access vlan {{ 2000+20*v+q2+i|1700+v }}
 switchport mode access
 load-interval 60
< speed 100 >                                      ! Apply if speed auto-negotiation with device fails
< duplex full >                                    ! Apply only for speed 100
 ipv6 nd raguard attach-policy pp6_raguard_client
 ipv6 dhcp guard attach-policy pp6_dhcp_client
 no snmp trap link-status
 storm-control broadcast level 5.00
 storm-control multicast level 5.00
 no lldp transmit
 no lldp receive
 no cdp enable
 spanning-tree bpduguard enable
 service-policy output pm_att_queuing
 no shutdown
!
! Port option for chassis 1 - single-connected servers trunked, if any
!
interface GigabitEthernet1/0/{{ 1-23|47 }}
 description {{ server name }}/{{ cable ID }}/trunk</{{ local description }}>
 switchport trunk native vlan 4094
 switchport trunk allowed vlan {{ list of valid 1700+v,2000+20*v+q2+i VLANs }}
 switchport mode trunk
 switchport nonegotiate
 load-interval 60
< speed 100 >                                      ! Apply if speed auto-negotiation with device fails
< duplex full >                                    ! Apply only for speed 100
 ipv6 nd raguard attach-policy pp6_raguard_client
 ipv6 dhcp guard attach-policy pp6_dhcp_client
 no snmp trap link-status
 storm-control broadcast level 5.00
 storm-control multicast level 5.00
 no lldp transmit
 no lldp receive
 no cdp enable
 spanning-tree bpduguard enable
 service-policy output pm_att_queuing
 no shutdown
!
! Port option for chassis 1 - non-virtualized single firewall inside interface, if any
!
interface GigabitEthernet1/0/{{ 1-23|47 }}
 description {{ firewall name }}/{{ cable ID }}/{{ VRF name }}/inside</{{ local description }}>
 switchport access vlan {{ 2000+20*v+q2+i }}
 switchport mode access
 load-interval 60
< speed 100 >                                      ! Apply if speed auto-negotiation with device fails
< duplex full >                                    ! Apply only for speed 100
 ipv6 nd raguard attach-policy pp6_raguard_client
 ipv6 dhcp guard attach-policy pp6_dhcp_client
 storm-control broadcast level 5.00
 storm-control multicast level 5.00
 no lldp transmit
 no lldp receive
 no cdp enable
 spanning-tree bpduguard enable
< service-policy input pm_trust_dscp_att_cos >     ! Apply if infrastructure behind firewall supports standard QoS marking
 service-policy output pm_att_queuing
 no shutdown
!
! Port option for chassis 1 - VoIP gateway, if any
!
interface GigabitEthernet1/0/{{ 1-23|47 }}
 description {{ VG name }}/{{ cable ID }}</{{ local description }}>
 switchport access vlan {{ 2000+20*v+q2+i }}
 switchport mode access
 load-interval 60
< speed 100 >                                      ! Apply if speed auto-negotiation with VG fails
< duplex full >                                    ! Apply only for speed 100
 ipv6 nd raguard attach-policy pp6_raguard_client
 ipv6 dhcp guard attach-policy pp6_dhcp_client
 storm-control broadcast level 5.00
 storm-control multicast level 5.00
 service-policy output pm_att_queuing
 no shutdown
!
! Port option for chassis 1 - IP SLA transaction probe, if any
!
interface GigabitEthernet1/0/{{ 1-23|47 }}
 description {{ TP name }}/{{ cable ID }}/blue/IPSLA</{{ local description }}>
 switchport access vlan {{ one of server1-5 (v=0-4) 2000+20*v+q2+i VLANs }}
 switchport mode access
 load-interval 60
< speed 100 >                                      ! Apply if speed auto-negotiation with TP fails
< duplex full >                                    ! Apply only for speed 100
 ipv6 nd raguard attach-policy pp6_raguard_client
 ipv6 dhcp guard attach-policy pp6_dhcp_client
 spanning-tree bpduguard enable
 service-policy input pm_att_classes_ipsla_data
 service-policy output pm_att_queuing
 no shutdown
!
! Port option for chassis 1 - other network device blue interface, if any
!
interface GigabitEthernet1/0/{{ 1-23|47 }}
 description {{ device name }}/{{ cable ID }}/blue</{{ local description }}>
 switchport access vlan {{ one of server1-5 (v=0-4) 2000+20*v+q2+i VLANs }}
 switchport mode access
 load-interval 60
 ipv6 nd raguard attach-policy pp6_raguard_client
 ipv6 dhcp guard attach-policy pp6_dhcp_client
 storm-control broadcast level 5.00
 storm-control multicast level 5.00
 spanning-tree bpduguard enable
< service-policy input pm_trust_dscp_att_cos >     ! Apply if device supports standard QoS marking
< service-policy input pm_att_classes_mgmt >       ! Apply if device does NOT support standard QoS marking
 service-policy output pm_att_queuing
 no shutdown
!
! Port option for chassis 1 - unused port, if any
!
interface GigabitEthernet1/0/{{ 1-23|47 }}
 description ** Not In Use **
 shutdown
 no snmp trap link-status
!
! End of port options for chassis 1
!
interface GigabitEthernet1/0/{{ 24|48}
 description ** RESERVED-SPAN **
 switchport mode access
 shutdown
 no snmp trap link-status
!
interface GigabitEthernet1/1/1
 description ** RESERVED **
 shutdown
 no snmp trap link-status
!
interface GigabitEthernet1/1/2
 description ** RESERVED **
 switchport mode access
 shutdown
 no snmp trap link-status
!
interface TenGigabitEthernet1/1/3
 description {{ site prefix }}/vSA{{ i }}-{{ vSD{{ n }}a|vOD{{ n }}a|eCDa|eND }}/trunk</{{ local description }}>
 switchport trunk native vlan 4094
 switchport trunk allowed vlan {{ list of valid 1900+i,2000+20*v+q2+i VLANs }}
 switchport mode trunk
 switchport nonegotiate
 load-interval 60
 ipv6 nd raguard attach-policy pp6_raguard_trust
 ipv6 dhcp guard attach-policy pp6_dhcp_trust
 udld port {{ aggressive|disable }}                    ! Aggressive if other end is Cisco and supports UDLD, else disable
 service-policy input pm_trust_dscp_att_cos
 service-policy output pm_att_queuing
 no shutdown
!
! 2nd uplink options - apply one option only
!
! Option 1 - uplink to vOD{{ n }}b/eCDb
!
interface TenGigabitEthernet1/1/4
 description {{ site prefix }}/vSA{{ i }}-{{ vSD{{ n }}b|vOD{{ n }}b|eCDb }}/trunk</{{ local description }}>
 switchport trunk native vlan 4094
 switchport trunk allowed vlan {{ list of valid 1900+i,2000+20*v+q2+i VLANs }}
 switchport mode trunk
 switchport nonegotiate
 load-interval 60
 ipv6 nd raguard attach-policy pp6_raguard_trust
 ipv6 dhcp guard attach-policy pp6_dhcp_trust
 udld port {{ aggressive|disable }}                    ! Aggressive if other end is Cisco and supports UDLD, else disable
 service-policy input pm_trust_dscp_att_cos
 service-policy output pm_att_queuing
 no shutdown
!
! Option 2 - no 2nd uplink to eND
!
interface TenGigabitEthernet1/1/4
 description ** RESERVED **
 shutdown
 no snmp trap link-status
!
! End of 2nd uplink options
!
interface Vlan1
 no ip address
 shutdown
!
! Repeat the following yellow edge VLAN interface definition (v=10-19) for each matching VLAN present
!
interface Vlan{{ 1700+v }}
 description {{ site prefix }}/vSA{{ i }}/yellow-edge{{ v-9 }}/{{ a.b.c.s+0 }}/{{ mm }}/{{ zone description }}
 no ip address
 service-policy input pm_att_classes_data
 no shutdown
!
! Repeat the following green edge VLAN interface definition (v=20-29) for each matching VLAN present
!
interface Vlan{{ 1700+v }}
 description {{ site prefix }}/vSA{{ i }}/green-edge{{ v-19 }}/{{ a.b.c.s+0 }}/{{ mm }}/{{ zone description }}
 no ip address
 service-policy input pm_att_classes_data
 no shutdown
!
! Repeat the following blue edge VLAN interface definition (v=30-39) for each matching VLAN present
!
interface Vlan{{ 1700+v }}
 description {{ site prefix }}/vSA{{ i }}/blue-edge{{ v-29 }}/{{ a.b.c.s+0 }}/{{ mm }}/{{ zone description }}
 no ip address
 service-policy input pm_att_classes_data
 no shutdown
!
interface Vlan{{ 1900+i }}
 description {{ site prefix }}/{{ vSD{{ n }}|vOD{{ n }}|eCD|eND }}-vSA{{ i }}/global/9.{{ b1.z2.aslot+0 }}/29/xA_mgmt</{{ local description }}>
 ip address 9.{{ b1.z2.aslot+5 }} 255.255.255.248
 no ip redirects
 no ip proxy-arp
 load-interval 60
 arp timeout 420
 service-policy input pm_att_classes_mgmt
 no shutdown
!
! Apply the following section for vSD VLAN type profile 2, if used on connected vSD
!
! Repeat the following blue data VLAN interface definition (v=0-19) for each matching VLAN present
!
interface Vlan{{ 2000+20*v+q2+i }}
 description {{ site prefix }}/vSD{{ n }}-vSA{{ i }}/blue-{{ v+1 }}</{{ VRF name }}>/9.{{ b2.c.s+0 }}/{{ mm }}</{{ local description }}>
 no ip address
 service-policy input pm_att_classes_voip_data
 no shutdown
!
! Repeat the following green/yellow/gray data VLAN inteface definition (v=20-39) for each matching VLAN present
!
interface Vlan{{ 2000+20*v+q2+i }}
 description {{ site prefix }}/vSD{{ n }}-vSA{{ i }}/{{ green|yellow|gray }}-{{ v-19 }}/{{ VRF name }}/{{ a.b.c.s+0 }}/{{ mm }}/{{ zone description }}</{{ local description }}>
 no ip address
 service-policy input pm_att_classes_data
 no shutdown
!
! Repeat the following blue data VLAN interface definition (v=40-44) for each matching VLAN present
!
interface Vlan{{ 2000+20*v+q2+i }}
 description {{ site prefix }}/vSD{{ n }}-vSA{{ i }}/blue-{{ v+1 }}</{{ VRF name }}>/9.{{ b2.c.s+0 }}/{{ mm }}</{{ local description }}>
 no ip address
 service-policy input pm_att_classes_voip_data
 no shutdown
!
! Repeat the following backend VLAN interface definition (v=45-46) for each matching VLAN present
!
interface Vlan{{ 2000+20*v+q2+i }}
 description {{ site prefix }}/vSD{{ n }}-vSA{{ i }}/back-{{ v-44 }}/local1/{{ a.b.c.s+0 }}/{{ mm }}</{{ local description }}>
 no ip address
 service-policy input pm_att_classes_data
 no shutdown
!
! Apply the following global device management VLAN interface definition (v=49), if present
!
interface Vlan{{ 2000+20*v+q2+i }}
 description {{ site prefix }}/vSD{{ n }}-vSA{{ i }}/global-1/{{ a.b.c.s+0 }}/{{ mm }}/dev_mgmt</{{ local description }}>
 no ip address
 service-policy input pm_att_classes_mgmt
 no shutdown
!
! End of section for vSD VLAN type profile 2vOD-eCD-eND VLAN type profile
!
! Apply the following section for vOD-eCD-eND VLAN type profile, if used on connected vOD, eCD or eND
!
! Repeat the following blue data VLAN interface definition (v=0-4) for each matching VLAN present
!
interface Vlan{{ 2000+20*v+q2+i }}
 description {{ site prefix }}/{{ vOD{{ n }}|eCD|eND }}-vSA{{ i }}/blue-{{ v+1 }}</{{ VRF name }}>/9.{{ b2.c.s+0 }}/{{ mm }}</{{ local description }}>
 no ip address
 service-policy input pm_att_classes_voip_data
 no shutdown
!
! Repeat the following green/yellow/gray data VLAN interface definition (v=15-19) for each matching VLAN present
!
interface Vlan{{ 2000+20*v+q2+i }}
 description {{ site prefix }}/{{ vOD{{ n }}|eCD|eND }}-vSA{{ i }}/{{ green|yellow|gray }}-{{ v-14 }}/{{ VRF name }}/{{ a.b.c.s+0 }}/{{ mm }}/{{ zone description }}</{{ local description }}>
 no ip address
 service-policy input pm_att_classes_data
 no shutdown
!
! Apply the following global device management VLAN interface definition (v=24), if present
!
interface Vlan{{ 2000+20*v+q2+i }}
 description {{ site prefix }}/{{ vOD{{ n }}|eCD|eND }}-vSA{{ i }}/global-1/9.{{ b1.c.s+0 }}/{{ mm }}/dev_mgmt</{{ local description }}>
 no ip address
 service-policy input pm_att_classes_mgmt
 no shutdown
!
! End of section for vOD-eCD-eND VLAN type profile
!
ip default-gateway 9.{{ b1.z2.aslot+1 }}
no ip forward-protocol nd
no ip http server
no ip http secure-server
ip ftp source-interface Vlan{{ 1900+i }}
ip tftp source-interface Vlan{{ 1900+i }}
!
ip access-list standard al_vty_access_aspr
 remark /**********************************************
 remark Restrict VTY access
 remark ***********************************************
 remark Site network infrastructure ranges
 remark **********************************************/
 permit 9.{{ b1.x }}.0 0.0.{{ 255-mmm }}.255                       ! Global management first range
 !
 ! Repeat the following entry for each additional global management allocation, if any
 !
 permit 9.{{ b1.y }}.0 0.0.{{ 255-mmm }}.255                       ! Global management additional range
 !
 remark /*********************************************-
 remark AT&T iGEMS management segments
 remark *********************************************-/
 permit 135.89.45.0 0.0.0.255
 permit 135.89.84.128 0.0.0.127
 permit 135.89.176.0 0.0.0.255
 permit 135.89.213.0 0.0.0.255
 remark /********************************************--
 remark Global AT&T enclave address pool
 remark ********************************************--/
 permit 9.65.6.0 0.0.1.255
 permit 9.78.8.0 0.0.7.255
 permit 9.78.16.0 0.0.15.255
 permit 9.78.32.0 0.0.15.255
 deny   any log
!
ip access-list extended al_att_class1
 remark /**********************************************
 remark QoS class 1 - classification policy
 remark Real-time traffic
 remark **********************************************/
 remark -
 remark Exclude VoiceSuite/softclient from class 1 treatment
 deny   udp any any range 5004 5005
 !
 ! Apply the following section if any Voice Gateways are present on this switch
 !
 remark -
 remark Voice Gateway
 !
 ! Repeat the following entry for each Voice Gateway present
 !
 permit udp host 9.{{ b.c.Voice Gateway }} range 16384 32767 any
 !
 ! End Voice Gateway section
 !
 ! Apply the following section if Siemens HiPath 8000 servers are present on this switch
 !
 remark -
 remark HiPath servers
 !
 ! Repeat the following entry for each HiPath server present
 !
 permit udp host 9.{{ b.c.HiPath server }} range 5100 6635 any
 !
 ! End HiPath section
 !
 deny   ip any any
!
! Class 2 forward and server return ACL options - apply one option only
!
! Option 1 - Americas
!
ip access-list extended al_att_data_forward_class2
 remark /**************************************************
 remark QoS class 2 - data forward classification policy
 remark **************************************************/
 permit tcp any 9.63.238.0 0.0.0.255 range 3200 3399  ! Blue Harmony SAP
 permit tcp any 9.63.238.0 0.0.0.255 range 3600 3699  ! Blue Harmony SAP
