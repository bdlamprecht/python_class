 permit tcp host 9.{{ b.c.remote subscriber server }} eq 1515 any                ! Dbase node replication
 permit tcp host 9.{{ b.c.remote subscriber server }} eq 2551 any                ! IntraCluster Active/Backup Polling
 permit tcp host 9.{{ b.c.remote subscriber server }} eq 2552 any                ! Subscriber Dbase Change Notification
 permit tcp host 9.{{ b.c.remote subscriber server }} range 2555 2556 any        ! Real Time Info Services Dbase
 permit udp host 9.{{ b.c.remote subscriber server }} eq 3000 any                ! CUCM Receive Change Notification
 permit tcp host 9.{{ b.c.remote subscriber server }} eq 4040 any                ! DRF Master Agent
 permit tcp host 9.{{ b.c.remote subscriber server }} eq 4343 any                ! DRF Local Agent
 permit tcp host 9.{{ b.c.remote subscriber server }} range 5001 5010 any        ! SOAP Monitor
 permit tcp host 9.{{ b.c.remote subscriber server }} eq 5555 any                ! License Manager Requests
 permit tcp host 9.{{ b.c.remote subscriber server }} eq 6970 any                ! Alternate TFTP
 permit tcp host 9.{{ b.c.remote subscriber server }} eq 7070 any                ! Certificate Manager Service
 permit tcp host 9.{{ b.c.remote subscriber server }} eq 7727 any                ! Application Dbase Change Notification
 permit tcp host 9.{{ b.c.remote subscriber server }} range 8001 8004 any        ! IntraCluster Comms Service
 permit tcp host 9.{{ b.c.remote subscriber server }} eq 8009 any                ! Tomcat Requests
 permit udp host 9.{{ b.c.remote subscriber server }} eq 8500 any                ! IntraCluster Ipsec Cluster Manager
 permit tcp host 9.{{ b.c.remote subscriber server }} range 8888 8889 any        ! RIS Service Manager Request
 !
 ! Repeat the following entries for each voice server address range utilizing CUCM Split Cluster, if any
 !
 permit tcp host 9.{{ b.c.remote split cluster server }} range 1500 1501 any     ! Dbase connection
 permit tcp host 9.{{ b.c.remote split cluster server }} eq 2551 any             ! IntraCluster Communication
 permit tcp host 9.{{ b.c.remote split cluster server }} range 2555 2556 any     ! Real Time Info Services Dbase
 permit tcp host 9.{{ b.c.remote split cluster server }} range 8001 8005 any     ! IntraCluster Comms Service
 permit tcp host 9.{{ b.c.remote split cluster server }} eq 9004 any             ! IntraCluster Communication
 !
 ! Repeat the following entries for each Hicom server present on this switch, if any
 !
 permit tcp host 9.{{ b.c.Hicom server }} eq 4060 any
 permit tcp host 9.{{ b.c.Hicom server }} range 30000 30199 any
 !
 ! Repeat the following entry for each voice device present utilizing Avaya H323 IP trunks, if any
 !
 permit tcp host 9.{{ b.c.voice device }} range 5075 5078 any                    ! Avaya H323 IP Trunks
 !
 deny   ip any any
