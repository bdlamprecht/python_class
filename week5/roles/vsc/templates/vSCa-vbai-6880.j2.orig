 address-family ipv4 vrf [VRF name]
  import path selection bestpath
  import path limit 4
  bgp router-id [a4.b4.x4.vslot1+9]
  !
  ! Repeat the following command for each additional aggregate of VRF site allocation, if any
  !
  aggregate-address [a4.b4.c4.s+0] 255.255.[mmm.mmm] summary-only attribute-map rm_site_summary_[VRF name]
  !
  ! Apply the following command for the WAN VPN infrastructure slot aggregate, if not included in site aggregate
  !
  aggregate-address [a4.b4.x4.vslot1+0] 255.255.255.224 summary-only attribute-map rm_site_summary_[VRF name]
  !
  ! Apply the following command for the first aggregate of VRF site allocation, if doing site-level aggregation
  !
  aggregate-address [a4.b4.c4.s+0] 255.255.[mmm.mmm] summary-only attribute-map rm_site_summary_[VRF name]
  !
  redistribute connected route-map rm_conn_to_bgp_vrf
  redistribute static route-map rm_static_to_bgp_vrf
  neighbor ce-[VRF prefix] peer-group
  neighbor ce-[VRF prefix] remote-as [assigned CE AS #]
  neighbor ce-[VRF prefix] description ** [site prefix]-CE-[CE VPN name] **
  neighbor ce-[VRF prefix] password [External BGP Password]
  neighbor ce-[VRF prefix] send-community
<  neighbor ce-[VRF prefix] remove-private-as >    ! Apply if VPAS is used by vID
  neighbor ce-[VRF prefix] route-map rm_from_ce_stub in
  neighbor ce-[VRF prefix] route-map rm_to_ce out
  neighbor ce-[VRF prefix] unsuppress-map rm_unsup_to_ce
  neighbor [a4.b4.x4.vslot1+0] peer-group ce-[VRF prefix]
  neighbor [a4.b4.x4.vslot1+0] description ** [site prefix]-ce-a-[CE VPN name] **
  neighbor [a4.b4.x4.vslot1+0] activate
  neighbor [a4.b4.x4.vslot1+4] peer-group ce-[VRF prefix]
  neighbor [a4.b4.x4.vslot1+4] description ** [site prefix]-ce-b-[CE VPN name] **
  neighbor [a4.b4.x4.vslot1+4] activate
  maximum-paths 2
  maximum-paths ibgp 2
  default-information originate
 exit-address-family
 !
 !----------------------------------------------------------------------------------------
 ! VRF address-family for Transport VPN using CE + WAN VPN infrastructure slots, if any
 !----------------------------------------------------------------------------------------
 !
 address-family ipv4 vrf [VRF name]
  import path selection bestpath
  import path limit 4
  bgp router-id [a4.b4.x4.vslot1+9]
  !
  ! Repeat the following command for each additional aggregate of VRF/multi-VRF site allocation, if any
  !
  aggregate-address [a4.b4.c4.s+0] 255.255.[mmm.mmm] summary-only attribute-map rm_site_summary_[VRF name]
  !
  ! Apply the following command for the CE + WAN VPN infrastructure slots aggregate, if not included in site aggregate
  !
  aggregate-address [a4.b4.x4.vslot+0] 255.255.255.192 summary-only attribute-map rm_site_summary_[VRF name]
  !
  ! Apply the following command for the first aggregate of VRF/multi-VRF site allocation, if doing multi-VRF or site-level aggregation
  !
  aggregate-address [a4.b4.c4.s+0] 255.255.[mmm.mmm] summary-only attribute-map rm_site_summary_[VRF name]
  !
  redistribute connected route-map rm_conn_to_bgp_vrf
  redistribute static route-map rm_static_to_bgp_vrf
  neighbor ce-[VRF prefix] peer-group
  neighbor ce-[VRF prefix] remote-as [assigned CE AS #]
  neighbor ce-[VRF prefix] description ** [site prefix]-CE-[CE VPN name] **
  neighbor ce-[VRF prefix] password [External BGP Password]
  neighbor ce-[VRF prefix] send-community
<  neighbor ce-[VRF prefix] remove-private-as >    ! Apply if VPAS is used by vID
  neighbor ce-[VRF prefix] route-map rm_from_ce in
  neighbor ce-[VRF prefix] route-map rm_to_ce out
  neighbor ce-[VRF prefix] unsuppress-map rm_unsup_to_ce
  neighbor [a4.b4.x4.vslot1+0] peer-group ce-[VRF prefix]
  neighbor [a4.b4.x4.vslot1+0] description ** [site prefix]-ce-a-[CE VPN name] **
  neighbor [a4.b4.x4.vslot1+0] activate
  neighbor [a4.b4.x4.vslot1+4] peer-group ce-[VRF prefix]
  neighbor [a4.b4.x4.vslot1+4] description ** [site prefix]-ce-b-[CE VPN name] **
  neighbor [a4.b4.x4.vslot1+4] activate
  maximum-paths 2
  maximum-paths ibgp 2
  default-information originate
 exit-address-family
