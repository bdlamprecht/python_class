hostname [site prefix]-vsc-a
!
boot-start-marker
boot system bootdisk:c6880x-adventerprisek9-mz.SPA.152-1.SY1a.bin
boot system bootdisk:
boot-end-marker
!
! Apply one of the following VRF definitions for each VRF required - order by VRF name A-Z
!
! VRF for global Yellow Access VPN, if required
!
vrf definition acc.y1
 description IGA Yellow Access Network - Global VPN - [WAN service] [ICORE VPN Name]
 rd 9.[b1.x1].5:210
 route-target export 8:210
 route-target import 8:210
 !
 address-family ipv4
  import map rm_from_vpn_af
 exit-address-family
!
! VRF for global Blue VPN - apply always
!
vrf definition blue
 description IGA Internal Network Blue - Global VPN - [WAN service] [ICORE VPN Name]
 rd 9.[b1.x1].5:100
 route-target export 8:100
 route-target import 8:100
 !
 address-family ipv4
  import map rm_from_vpn_af
 exit-address-family
 !
 address-family ipv6
  import map rm_from_vpn_af
 exit-address-family
!
! VRF for global Transport VPN, if any
!
vrf definition [VRF name]
 description [VRF description] - Global Transport VPN - [WAN service] [ICORE VPN Name]
 rd 9.[b1.x1].5:[VPNID]
 route-target export 8:[VPNID]
 route-target import 8:[VPNID]
 !
 address-family ipv4
  import map rm_from_vpn_af
 exit-address-family
!
! VRF for global/regional Global Stub VPN, if any
!
vrf definition [VRF name]
 description [VRF description] - [Global|Regional] Stub VPN - [WAN service] [ICORE VPN Name]
 rd 9.[b1.x1].5:[VPNID]
 route-target export 8:[VPNID]
 route-target import 8:[VPNID]
 !
 address-family ipv4
  import map rm_from_vpn_af
 exit-address-family
!
! End of VRF definitions
